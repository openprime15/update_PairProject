    <title>MY SAFE SQUARE (CHOI, KANG)</title>

    <!-- Bootstrap core CSS -->
    <link href="../css/main.css" rel="stylesheet">

    <!-- Custom styles for this template -->
    <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/3.4.1/css/bootstrap.min.css">
    <link href="../css/cover.css" rel="stylesheet">
    <script src="https://maxcdn.bootstrapcdn.com/bootstrap/3.4.1/js/bootstrap.min.js"></script>

    <!-- <script src="https://ajax.googleapis.com/ajax/libs/jquery/3.4.1/jquery.min.js"></script> -->
    <script src="//code.jquery.com/jquery-1.11.1.min.js"></script>
    <script src="//maxcdn.bootstrapcdn.com/bootstrap/3.3.0/js/bootstrap.min.js"></script>
    <script src="../js/client.js"></script>
    <style id="mystyle"></style>
    <style>
    #c_comments
    {
        height: 100px;
    }
  </style>
  </head>

  <body class="text-center">

    <div class="cover-container d-flex h-100 p-3 mx-auto flex-column">
      <header class="masthead mb-auto">
        <div class="inner">
          <h3 class="masthead-brand">Cover</h3>
          <nav class="nav nav-masthead justify-content-center">
            <a class="nav-link active" href="#" onclick="movehome()">Home</a>
            
            <a id="report_view_btn" class="nav-link active">Board</a>
            <a class="nav-link active" href="#" onclick="movecontact()">Contact</a>
          </nav>
        </div>
      </header>
    
      <div id="content_div"></div>
        <main id="main" role="main" class="inner cover">
          <div class="mx-auto p-3 bg-dark text-left" style="width:600px;">
            <h2>Safe Square CONTACT</h2>
            <form action="main" method='get' class="needs-validation" novalidate>
              <div class="form-group">
                <label for="uname">이름(name):</label>
                <input type="text" class="form-control" id="c_name" placeholder="Your name" name="name" required>
                <div class="valid-feedback">구웃.</div>
                <div class="invalid-feedback">Please fill out this field.</div>
              </div>
              <div class="form-group">
                <label for="email">이메일(E-mail):</label>
                <input type="text" class="form-control" id="c_email" placeholder="Your E-mail" name="email" required>
                <div class="valid-feedback">Valid.</div>
                <div class="invalid-feedback">Please fill out this field.</div>
              </div>
              <div class="form-group">
                <label for="phone">전화번호(Phone Number):</label>
                <input type="password" class="form-control" id="c_phone" placeholder="번호를 입력해주세요" name="phone" required>
                <div class="valid-feedback">좋아요.</div>
                <div class="invalid-feedback">Please fill out this field.</div>
              </div>
              <div class="form-group">
                <label for="comments">내용(comments):</label>
                <input type="text" class="form-control" id="c_comments" name="pswd" required>
                <div class="valid-feedback">완벽합니다.</div>
                <div class="invalid-feedback">Please fill out this field.</div>
              </div>
              <input type="button" id="c_submit" class="btn btn-primary btn btn-default" value="제출합니다">
            </form>
          </div>
        </main>
      
      <footer class="mastfoot mt-auto">
        <div class="inner">
          <p>Cover template for <a href="https://getbootstrap.com/">Bootstrap</a>, by <a href="https://twitter.com/mdo">@mdo</a>.</p>
        </div>
      </footer>
    </div>

  </body>
  <script type="text/javascript">
    function movehome(){
      location.href="/";
    }
  </script>
    <script type="text/javascript">
      function movecontact(){
        location.href="/contact/here";
      }
    </script>
</html>
